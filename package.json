{
  "name": "feathers-utils",
  "version": "3.0.0",
  "description": "Some utils for projects using '@feathersjs/feathers'",
  "author": "fratzinger",
  "repository": {
    "type": "git",
    "url": "https://github.com/fratzinger/feathers-utils"
  },
  "engines": {
    "node": ">= 16"
  },
  "homepage": "https://github.com/fratzinger/feathers-utils",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "lib/**",
    "dist/**"
  ],
  "scripts": {
    "build": "unbuild",
    "version": "npm run build",
    "release": "np",
    "test": "vitest run",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.5",
    "@feathersjs/commons": "^5.0.5",
    "@feathersjs/errors": "^5.0.5",
    "fast-equals": "^5.0.1",
    "feathers-hooks-common": "^7.0.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@feathersjs/feathers": "^5.0.5",
    "@feathersjs/memory": "^5.0.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitest/coverage-c8": "^0.31.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "np": "^8.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vitest": "^0.31.4"
  },
  "peerDependencies": {
    "@feathersjs/feathers": "^5.0.0"
  }
}
