{
  "name": "feathers-utils",
  "version": "6.0.0",
  "description": "Some utils for projects using '@feathersjs/feathers'",
  "author": "fratzinger",
  "repository": {
    "type": "git",
    "url": "https://github.com/fratzinger/feathers-utils"
  },
  "engines": {
    "node": ">= 18"
  },
  "homepage": "https://github.com/fratzinger/feathers-utils",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "lib/**",
    "dist/**"
  ],
  "scripts": {
    "build": "unbuild",
    "version": "npm run build",
    "release": "np",
    "test": "vitest run",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.31",
    "@feathersjs/errors": "^5.0.31",
    "@feathersjs/feathers": "^5.0.31",
    "fast-equals": "^5.0.1",
    "feathers-hooks-common": "^8.2.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@feathersjs/memory": "^5.0.31",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "np": "^10.1.0",
    "prettier": "^3.4.1",
    "shx": "^0.3.4",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@feathersjs/feathers": "^5.0.0"
  }
}
